generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Analysis {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  title     String
  data      Json
  insights  String?
}

model UserProfile {
  id        String   @id @default(cuid())
  name      String
  role      String
  avatarUrl String?
}
