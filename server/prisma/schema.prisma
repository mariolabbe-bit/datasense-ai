generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String     @id @default(cuid())
  email     String     @unique
  password  String
  name      String?
  analyses  Analysis[]
  createdAt DateTime   @default(now())
}

model Analysis {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  title     String
  data      Json
  insights  String?
  userId    String
  user      User     @relation(fields: [userId], references: [id])
}
